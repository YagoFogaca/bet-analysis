// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Time {
  id             String @unique
  name           String
  points         Int
  victory        Int
  defeat         Int
  draw           Int
  goals_scored   Int
  goals_conceded Int
  corners        Int
  off_side       Int
  yellow_cards   Int
  red_cards      Int
}
